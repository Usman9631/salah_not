{
  "build": {
    "development": {
      "distribution": "internal",
      "channel": "development",
      "developmentClient": true,
      "android": { 
        "gradleCommand": ":app:assembleDebug",
        "env": {
          "GOOGLE_SERVICES_JSON": "android/app/google-services.json"
        }
      },
      "ios": { "simulator": true }
    },
    "preview": {
      "distribution": "internal",
      "channel": "preview"
    },
    "production": {
      "channel": "production"
    }
  }
}
